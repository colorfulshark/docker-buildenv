# /usr/bin/env bash

SCRIPTPATH="$( cd "$(dirname "$0")" >/dev/null 2>&1 ; pwd -P )"
source $SCRIPTPATH/config

# find project configure file
if [ "x$1" != "x" ] && [ -f "$config_path/$1" ]; then
	project=$config_path/$1
else
	echo 'please specify a valid project'
	exit 1
fi

source $project

$SCRIPTPATH/cs -i $docker_image -n $target -c $project $volume
